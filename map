    <script>
      function initMap() {
        var uluru = {lat: -25.363, lng: 131.044};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 4,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
    <script async defer
    src="http://media.gq.com/photos/57ffebd7e5dad068275d781e/master/w_2000/1616-gq-fekl12-01-kl-gqstyle.jpg = myMap">
    </script>
    
